ucts\n            } else {\n                this.filteredProducts = this.products.filter((product) => product.categories.includes(category.value));\n            }\n        },\n        changeCollection(collection) {\n            if (collection.value === 'all') {\n                this.filteredProducts = this.products\n            } else {\n                this.filteredProducts = this.products.filter((product) => product.collection.name === collection.value);\n            }\n        },\n        changeSaleType(type) {\n            if (type.value === 'all') {\n                this.filteredProducts = this.products\n            } else {\n                this.filteredProducts = this.products.filter((product) => product.saleType === type.value);\n            }\n        },\n        onChangeRange(value) {\n            this.priceRange = value;\n        },\n        filterPrice() {\n            if (this.priceRange.length) {\n                this.filteredProducts = this.products.filter(product => {\n                    const startRange = this.priceRange[0],\n                        endRange = this.priceRange[1]\n                    return product.price.amount >= startRange && product.price.amount <= endRange\n                });\n            }\n        }\n    },\n    mounted() {\n        this.filteredProducts = this.products;\n    }\n}"],"mappings":"AAAA,OAAOA,YAAY,MAAM,uBAAuB;AAEhD,eAAe;EACXC,MAAM,EAAE,CAACD,YAAY,CAAC;EACtBE,IAAI,GAAG;IACH,OAAO;MACHC,gBAAgB,EAAE,EAAE;MACpBC,UAAU,EAAE;IAChB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,WAAW,CAACC,IAAI,EAAE;MACd,IAAI,CAACJ,gBAAgB,GAAG,IAAI,CAACK,QAAQ,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACjD,IAAIJ,IAAI,CAACK,KAAK,KAAK,YAAY,EAAE;UAC7B,OAAOF,CAAC,CAACG,OAAO,GAAGF,CAAC,CAACE,OAAO,GAAG,CAAC,GAAG,CAAC,CAAC;QACzC;QACA,OAAOH,CAAC,CAACG,OAAO,GAAGF,CAAC,CAACE,OAAO,GAAG,CAAC,GAAG,CAAC,CAAC;MACzC,CAAC,CAAC;IACN,CAAC;IACDC,cAAc,CAACC,QAAQ,EAAE;MACrB,IAAIA,QAAQ,CAACH,KAAK,KAAK,KAAK,EAAE;QAC1B,IAAI,CAACT,gBAAgB,GAAG,IAAI,CAACK,QAAQ;MACzC,CAAC,MAAM