rrent","methods","onClose","currentHandler","item","index","$emit"],"sourceRoot":"","sources":["/Users/ozgunozerk/Developer/hackathon-costa-rica/nuron-vuejs/src/components/select/NiceSelect.vue"],"sourcesContent":["<template>\n    <div :class=\"[`nice-select ${className}`, {'open': open}]\"\n         tabindex=\"0\"\n         @click.prevent=\"open = !open\"\n         role=\"button\"\n         ref=\"\">\n        <span class=\"current\">{{ current?.text || placeholder }}</span>\n        <ul class=\"list\" role=\"menubar\" @click.prevent=\"$event.stopPropagation()\">\n            <li :class=\"[`option`, {'selected focus': item.value === current.value}]\"\n                v-for=\"(item, index) in options\"\n                :key=\"`option-${index}`\"\n                @click.prevent=\"currentHandler(item, index)\"\n                role=\"menuitem\">\n                {{ item.text }}\n            </li>\n        </ul>\n    </div>\n</template>\n\n<script>\n    export default {\n        props: {\n            options: '',\n            defaultCurrent: {},\n            placeholder: {},\n            className: {},\n            name: {}\n        },\n        name: 'NiceSelect',\n        data() {\n            return {\n                open: false,\n                current: this.options[this.defaultCurrent],\n            }\n        },\n        methods: {\n            onClose() {\n                this.open = false\n            },\n            currentHandler(item, index) {\n                this.current = this.options[index];\n                this.$emit('onChange', item)\n                this.onClose()\n            }\n        }\n    }\n</script>"]}]}