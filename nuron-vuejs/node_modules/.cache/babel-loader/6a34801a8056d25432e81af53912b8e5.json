log: {}\n            }\n        },\n        methods: {\n            getBlog(blogId) {\n                this.blog = this.posts.find(item => item.id == blogId);\n            }\n        },\n        created() {\n            this.getBlog(this.id);\n        },\n        watch: {\n            '$route.params.id': function (val) {\n                this.id = val;\n                this.getBlog(this.id);\n            }\n        },\n    }\n</script>"]}]}