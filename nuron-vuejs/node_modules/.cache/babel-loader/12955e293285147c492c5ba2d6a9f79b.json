CT,IAAIA,GAAE,IAAK,KAAKA,GAAE,GAAI,IAAI,CAACN,SAAS,EAAE;QAClC;MACJ;MACA,IAAI,CAACJ,QAAO,GAAIU,GAAG;MACnB,IAAI,IAAI,CAACb,aAAa,EAAE;QACpB,IAAI,CAACc,KAAK,CAAC,eAAe,CAAC;MAC/B,OAAO;QACHC,MAAM,CAACC,QAAQ,CAAC,CAAC,EAAE,CAAC,CAAC;MACzB;MACA,IAAI,CAACF,KAAK,CAAC,eAAe,EAAE,IAAI,CAACT,YAAY,EAAE,IAAI,CAACC,SAAS,EAAE,IAAI,CAACZ,WAAW,CAAC;IACpF;EACJ,CAAC;EACDuB,OAAO,GAAG;IACN,IAAI,CAACH,KAAK,CAAC,eAAe,EAAE,IAAI,CAACT,YAAY,EAAE,IAAI,CAACC,SAAS,EAAE,IAAI,CAACZ,WAAW;EACnF;AACJ","names":["name","props","posts","countOfPage","type","Number","default","paginationClass","String","scrollControl","Boolean","data","currPage","computed","filteredRows","pageStart","totalPage","Math","ceil","length","methods","setPage","idx","$emit","window","scrollTo","mounted"],"sourceRoot":"","sources":["/Users/ozgunozerk/Developer/hackathon-costa-rica/nuron-vuejs/src/components/pagination/BlogPagination.vue"],"sourcesContent":["<template>\n    <nav v-if=\"posts.length > 8\"\n         class=\"pagination-wrapper\"\n         aria-label=\"Page navigation example\">\n        <ul :class=\"`pagination ${paginationClass}`\">\n            <li class=\"page-item\"\n                :class=\"{'disabled': (currPage === 1)}\"\n                @click.prevent=\"setPage(currPage-1)\">\n                <a class=\"page-link\" href=\"\">\n                    Previous\n                </a>\n            </li>\n            <li class=\"page-item\"\n                v-for=\"n in totalPage\"\n                @click.prevent=\"setPage(n)\">\n                <a :class=\"[`page-link`, {'active': (currPage === (n))}]\" href=\"\">\n                    {{ n }}\n                </a>\n            </li>\n            <li class=\"page-item\"\n                :class=\"{'disabled': (currPage === totalPage)}\"\n                @click.prevent=\"setPage(currPage + 1)\">\n                <a class=\"page-link\" href=\"\">\n                    Next\n                </a>\n            </li>\n        </ul>\n    </nav>\n</template>\n\n<script>\n    export default {\n        name: 'BlogPagination',\n        props: {\n            posts: {},\n            countOfPage: {\n                type: Number,\n                default: 8\n            },\n            paginationClass: {\n                type: String\n            },\n            scrollControl: {\n                type: Boolean,\n                default: false\n            }\n        },\n        data() {\n            return {\n                currPage: 1\n            }\n        },\n        computed: {\n            filteredRows() {\n                return this.posts;\n            },\n            pageStart() {\n                return (this.currPage - 1) * this.countOfPage;\n            },\n            totalPage() {\n                return Math.ceil(this.filteredRows.length / this.countOfPage);\n            }\n        },\n        methods: {\n            setPage(idx) {\n                if (idx <= 0 || idx > this.totalPage) {\n                    return;\n                }\n                this.currPage = idx;\n                if (this.scrollControl) {\n                    this.$emit('scrollControl');\n                } else {\n                    window.scrollTo(0, 0);\n                }\n                this.$emit('paginatedData', this.filteredRows, this.pageStart, this.countOfPage);\n            },\n        },\n        mounted() {\n            this.$emit('paginatedData', this.filteredRows, this.pageStart, this.countOfPage)\n        }\n    }\n</script>\n"]}]}