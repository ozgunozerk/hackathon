e\"\n                            @change=\"previewImage($event, 'coverPhoto')\"\n                            ref=\"coverPhotoInput\"\n                          />\n                          <label for=\"nipa\" title=\"No File Choosen\">\n                            <span class=\"text-center color-white\"\n                              >Upload Cover</span\n                            >\n                          </label>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n\n              <div\n                class=\"tab-pane fade\"\n                id=\"nav-homes\"\n                role=\"tabpanel\"\n                aria-labelledby=\"nav-home-tab\"\n              >\n                <div class=\"nuron-information\">\n                  <div class=\"profile-form-wrapper\">\n                    <div class=\"input-two-wrapper mb--15\">\n                      <div class=\"first-name half-wid\">\n                        <label for=\"contact-name\" class=\"form-label\"\n                          >First Name</label\n                        >\n                        <input id=\"contact-name\" type=\"text\" value=\"Mr.\" />\n                      </div>\n                      <div class=\"last-name half-wid\">\n                        <label for=\"contact-name-last\" class=\"form-label\"\n                          >Last Name</label\n                        >\n                        <input\n                          id=\"contact-name-last\"\n                          type=\"text\"\n                          value=\"Sunayra\"\n                        />\n                      </div>\n                    </div>\n                    <div class=\"email-area\">\n                      <label for=\"Email\" class=\"form-label\"\n                        >Edit Your Email</label\n                      >\n                      <input\n                        id=\"Email\"\n      