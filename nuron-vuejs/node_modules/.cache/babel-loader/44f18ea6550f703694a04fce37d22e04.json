e\"\n                            @change=\"previewImage($event, 'coverPhoto')\"\n                            ref=\"coverPhotoInput\"\n                          />\n                          <label for=\"nipa\" title=\"No File Choosen\">\n                            <span class=\"text-center color-white\"\n                              >Upload Cover</span\n                            >\n                          </label>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n\n              <div\n                class=\"tab-pane fade\"\n                id=\"nav-homes\"\n                role=\"tabpanel\"\n                aria-labelledby=\"nav-home-tab\"\n              >\n                <div class=\"nuron-information\">\n                  <div class=\"profile-form-wrapper\">\n                    <div class=\"input-two-wrapper mb--15\">\n                      <div class=\"first-name half-wid\">\n                        <label for=\"contact-name\" class=\"form-label\"\n                          >First Name</label\n                        >\n                        <input id=\"contact-name\" type=\"text\" value=\"Mr.\" />\n                      </div>\n                      <div class=\"last-name half-wid\">\n                        <label for=\"contact-name-last\" class=\"form-label\"\n                          >Last Name</label\n                        >\n                        <input\n                          id=\"contact-name-last\"\n                          type=\"text\"\n                          value=\"Sunayra\"\n                        />\n                      </div>\n                    </div>\n                    <div class=\"email-area\">\n                      <label for=\"Email\" class=\"form-label\"\n                        >Edit Your Email</label\n                      >\n                      <input\n                        id=\"Email\"\n                        type=\"email\"\n                        value=\"example@gmail.com\"\n                      />\n                    </div>\n                  </div>\n                  <div class=\"edit-bio-area mt--30\">\n                    <label for=\"description\" class=\"form-label\"\n                      >Edit Your Bio</label\n                    >\n                    <textarea id=\"description\">\nHello, I am Alamin, A Front-end Developer...</textarea\n                    >\n                  </div>\n                  <div class=\"input-two-wrapepr-prifile\">\n                    <div class=\"role-area mt--15\">\n                      <label for=\"Role\" class=\"form-label mb--10\"\n                        >Your Role</label\n                      >\n                      <input\n                        id=\"Role\"\n                        type=\"text\"\n                        value=\"Front-end Developer\"\n                      />\n                    </div>\n                    <select class=\"profile-edit-select\">\n                      <option selected>Select Your Gender</option>\n                      <option value=\"1\">Male</option>\n                      <option value=\"2\">Female</option>\n                      <option value=\"3\">Third Gender</option>\n                    </select>\n                  </div>\n\n                  <div class=\"input-two-wrapper mt--15\">\n                    <div class=\"half-wid currency\">\n                      <select class=\"profile-edit-select\">\n                        <option selected>Currency</option>\n                        <option value=\"1\">($)USD</option>\n                        <option value=\"2\">SSC</option>\n                        <option value=\"3\">BIT Coin</option>\n                      </select>\n                    </div>\n                    <div class=\"half-wid phone-number\">\n                      <label for=\"PhoneNumber\" class=\"form-label\"\n                        >Phone Number</label\n                      >\n                      <input\n                        id=\"PhoneNumber\"\n                        type=\"text\"\n                        value=\"+880100000000\"\n                      />\n                    </div>\n                  </div>\n                  <div class=\"input-two-wrapper mt--15\">\n                    <div class=\"half-wid currency\">\n                      <select class=\"profile-edit-select\">\n                        <option selected>Location</option>\n                        <option value=\"1\">United State</option>\n                        <option value=\"2\">Katar</option>\n                        <option value=\"3\">Canada</option>\n                      </select>\n                    </div>\n                    <div class=\"half-wid phone-number\">\n                      <label for=\"address\" class=\"form-label\">Address</label>\n                      <input id=\"address\" type=\"text\" value=\"USA Cidni\" />\n                    </div>\n                  </div>\n                  <div class=\"button-area save-btn-edit\">\n                    <a\n                      href=\"#\"\n                      class=\"btn btn-primary-alta mr--15\"\n                      @click=\"alert('Cancel Edit Profile?')\"\n                      >Cancel</a\n                    >\n                    <a\n                      href=\"#\"\n                      class=\"btn btn-primary\"\n                      @click=\"alert('Successfully Saved Your Profile?')\"\n                      >Save</a\n                    >\n                  </div>\n                </div>\n              </div>\n\n              <div\n                class=\"tab-pane fade\"\n                id=\"nav-profile\"\n                role=\"tabpanel\"\n                aria-labelledby=\"nav-profile-tab\"\n              >\n                <div class=\"nuron-information\">\n                  <div class=\"condition\">\n                    <h5 class=\"title\">Create Your Password</h5>\n                    <p class=\"condition\">\n                      Passwords are a critical part of information and network\n                      security. Passwords serve to protect user accounts but a\n                      poorly chosen password, if compromised, could put the\n                      entire network at risk.\n                    </p>\n                    <hr />\n                    <div class=\"email-area\">\n                      <label for=\"Email2\" class=\"form-label\">Enter Email</label>\n                      <input id=\"Email2\" type=\"email\" value=\"\" />\n                    </div>\n                  </div>\n                  <div class=\"input-two-wrapper mt--15\">\n                    <div class=\"old-password half-wid\">\n                      <label for=\"oldPass\" class=\"form-label\"\n                        >Enter Old Password</label\n                      >\n                      <input id=\"oldPass\" type=\"password\" />\n                    </div>\n                    <div class=\"new-password half-wid\">\n                      <label for=\"NewPass\" class=\"form-label\"\n                        >Create New Password</label\n                      >\n                      <input id=\"NewPass\" type=\"password\" />\n                    </div>\n                  </div>\n                  <div class=\"email-area mt--15\">\n                    <label for=\"rePass\" class=\"form-label\"\n                      >Confirm Password</label\n                    >\n                    <input id=\"rePass\" type=\"password\" />\n                  </div>\n                  <a\n                    href=\"#\"\n                    class=\"btn btn-primary save-btn-edit\"\n                    @click=\"alert('Successfully Changed Password?')\"\n                    >Save</a\n                  >\n                </div>\n              </div>\n\n              <div\n                class=\"tab-pane fade\"\n                id=\"nav-contact\"\n                role=\"tabpanel\"\n                aria-labelledby=\"nav-contact-tab\"\n              >\n                <div class=\"nuron-information\">\n                  <h5 class=\"title\">Make Sure Your Notification setting</h5>\n                  <p class=\"notice-disc\">\n                    Notification Center is where you can find app notifications\n                    and Quick Settings—which give you quick access to commonly\n                    used settings and apps. You can change your notification\n                    settings at any time from the Settings app. Select Start ,\n                    then select Settings\n                  </p>\n                  <hr />\n\n                  <div class=\"notice-parent-wrapper d-flex\">\n                    <div class=\"notice-child-wrapper\">\n                      <!-- single notice wrapper -->\n                      <div class=\"single-notice-setting\">\n                        <div class=\"input\">\n                          <input\n                            type=\"checkbox\"\n                            id=\"themeSwitch\"\n                            name=\"theme-switch\"\n                            class=\"theme-switch__input\"\n                          />\n                          <label for=\"themeSwitch\" class=\"theme-switch__label\">\n                            <span></span>\n                          </label>\n                        </div>\n                        <div class=\"content-text\">\n                          <p>Order Confirmation Notice</p>\n                        </div>\n                      </div>\n                      <!-- single notice wrapper End -->\n\n                      <!-- single notice wrapper -->\n                      <div class=\"single-notice-setting mt--15\">\n                        <div class=\"input\">\n                          <input\n                            type=\"checkbox\"\n                            id=\"themeSwitchs\"\n                            name=\"theme-switch\"\n                            class=\"theme-switch__input\"\n                          />\n                          <label for=\"themeSwitchs\" class=\"theme-switch__label\">\n                            <span></span>\n                          </label>\n                        </div>\n                        <div class=\"content-text\">\n                          <p>New Items Notification</p>\n                        </div>\n                      </div>\n                      <!-- single notice wrapper End -->\n\n                      <!-- single notice wrapper -->\n                      <div class=\"single-notice-setting mt--15\">\n                        <div class=\"input\">\n                          <input\n                            type=\"checkbox\"\n                            id=\"OrderNotice\"\n                            name=\"theme-switch\"\n                            class=\"theme-switch__input\"\n                          />\n                          <label for=\"OrderNotice\" class=\"theme-switch__label\">\n                            <span></span>\n                          </label>\n                        </div>\n                        <div class=\"content-text\">\n                          <p>New Bid Notification</p>\n                        </div>\n                      </div>\n                      <!-- single notice wrapper End -->\n\n                      <!-- single notice wrapper -->\n                      <div class=\"single-notice-setting mt--15\">\n                        <div class=\"input\">\n                          <input\n                            type=\"checkbox\"\n                            id=\"newPAy\"\n                            name=\"theme-switch\"\n                            class=\"theme-switch__input\"\n                          />\n                          <label for=\"newPAy\" class=\"theme-switch__label\">\n                            <span></span>\n                          </label>\n                        </div>\n                        <div class=\"content-text\">\n                          <p>Payment Card Notification</p>\n                        </div>\n                      </div>\n                      <!-- single notice wrapper End -->\n\n                      <!-- single notice wrapper -->\n                      <div class=\"single-notice-setting mt--15\">\n                        <div class=\"input\">\n                          <input\n                            type=\"checkbox\"\n                            id=\"EndBid\"\n                            name=\"theme-switch\"\n                            class=\"theme-switch__input\"\n                          />\n                          <label for=\"EndBid\" class=\"theme-switch__label\">\n                            <span></span>\n                          </label>\n                        </div>\n                        <div class=\"content-text\">\n                          <p>Ending Bid Notification Before 5 min</p>\n                        </div>\n                      </div>\n                      <!-- single notice wrapper End -->\n\n                      <!-- single notice wrapper -->\n                      <div class=\"single-notice-setting mt--15\">\n                        <div class=\"input\">\n                          <input\n                            type=\"checkbox\"\n                            id=\"Approve\"\n                            name=\"theme-switch\"\n                            class=\"theme-switch__input\"\n                          />\n                          <label for=\"Approve\" class=\"theme-switch__label\">\n                            <span></span>\n                          </label>\n                        </div>\n                        <div class=\"content-text\">\n                          <p>Notification for approving product</p>\n                        </div>\n                      </div>\n                      <!-- single notice wrapper End -->\n                    </div>\n                    <div class=\"notice-child-wrapper\"></div>\n                  </div>\n                  <button\n                    class=\"btn btn-primary save-btn-edit\"\n                    @click=\"\n                      alert('Successfully saved Your Notification setting')\n                    \"\n                  >\n                    Save\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </layout>\n</template>\n\n<script>\nimport Layout from \"@/components/layouts/Layout\";\nimport Breadcrumb from \"@/components/breadcrumb/Breadcrumb\";\nexport default {\n  name: 'EditProfilePage',\n  components: { Breadcrumb, Layout },\n  methods: {\n    previewImage(e, id) {\n      if (e.target.files && e.target.files.length > 0) {\n        const file = e.target.files[0];\n        document.querySelector(`#${id}`).setAttribute('src', URL.createObjectURL(file));\n      }\n    }\n  }\n}\n</script>\n"]}]}