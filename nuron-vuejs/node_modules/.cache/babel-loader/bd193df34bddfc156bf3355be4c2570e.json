ACnB,MAAMV,IAAG,GAAI,IAAI,CAACE,QAAQ;MAE1B,IAAI,CAAC,IAAI,CAACD,EAAE,CAACU,MAAM,EAAE;QACjBhB,KAAK,CAAC;UACFiB,MAAM,EAAE,MAAM;UACdC,GAAG,EAAE,2DAA2D;UAChEb;QACJ,CAAC,CAAC,CAACc,IAAI,CAAEC,IAAI,IAAK;UACd,IAAI,CAACT,SAAQ,GAAI,IAAI;UACrB,IAAI,CAACJ,QAAQ,CAACH,IAAG,GAAI,EAAE;UACvB,IAAI,CAACG,QAAQ,CAACJ,KAAI,GAAI,EAAE;UACxB,IAAI,CAACI,QAAQ,CAACC,OAAM,GAAI,EAAE;UAC1B,IAAI,CAACD,QAAQ,CAACE,OAAM,GAAI,EAAE;UAC1B,IAAI,CAACF,QAAQ,CAACG,UAAS,GAAI,KAAK;UAChC,IAAI,CAACJ,EAAE,CAACe,MAAM,EAAC;QACnB,CAAC,CAAC,CAACC,KAAK,CAAEC,GAAG,IAAK;UACdC,OAAO,CAACC,GAAG,CAACF,GAAG;QACnB,CAAC,CAAC;MACN,OAAO;QACHC,OAAO,CAACC,GAAG,CAAC,oCAAoC;MACpD;IACJ;EACJ;AACJ","names":["axios","useVuelidate","required","email","name","data","v$","formData","subject","message","allowTerms","submitted","validations","methods","submitForm","$validate","$error","method","url","then","_res","$reset","catch","err","console","log"],"sourceRoot":"","sources":["/Users/ozgunozerk/Developer/hackathon-costa-rica/nuron-vuejs/src/components/contact/ContactForm.vue"],"sourcesContent":["<template>\n    <form class=\"rwt-dynamic-form\" id=\"contact-form\">\n        <div class=\"mb-5\">\n            <label for=\"contact-name\" class=\"form-label\">Your Name</label>\n            <input name=\"contact-name\" id=\"contact-name\" type=\"text\" v-model=\"formData.name\">\n            <span v-if=\"v$.formData.name.$error\" class=\"text-danger mt-2 d-inline-block\">\n                {{ v$.formData.name.$errors[0].$message }}\n            </span>\n        </div>\n        <div class=\"mb-5\">\n            <label for=\"contact-email\" class=\"form-label\">Email</label>\n            <input id=\"contact-email\" name=\"contact-email\" type=\"email\" v-model=\"formData.email\">\n            <span v-if=\"v$.formData.email.$error\" class=\"text-danger mt-2 d-inline-block\">\n                {{ v$.formData.email.$errors[0].$message }}\n            </span>\n        </div>\n        <div class=\"mb-5\">\n            <label for=\"subject\" class=\"form-label\">Subject</label>\n            <input id=\"subject\" name=\"subject\" type=\"text\" v-model=\"formData.subject\">\n            <span v-if=\"v$.formData.subject.$error\" class=\"text-danger mt-2 d-inline-block\">\n                {{ v$.formData.subject.$errors[0].$message }}\n            </span>\n        </div>\n        <div class=\"mb-5\">\n            <label for=\"contact-message\" class=\"form-label\">Write Message</label>\n            <textarea name=\"contact-message\" id=\"contact-message\" rows=\"3\" v-model=\"formData.message\"></textarea>\n            <span v-if=\"v$.formData.message.$error\" class=\"text-danger mt-2 d-inline-block\">\n                {{ v$.formData.message.$errors[0].$message }}\n            </span>\n        </div>\n        <div class=\"mb-5 rn-check-box\">\n            <div>\n                <input id=\"condition\" type=\"checkbox\" class=\"rn-check-box-input\" v-model=\"formData.allowTerms\">\n                <label for=\"condition\" class=\"rn-check-box-label\">\n                    Allow to all terms & condition\n                </label>\n            </div>\n            <span v-if=\"v$.formData.allowTerms.$error\" class=\"text-danger mt-2 d-inline-block\">\n                {{ v$.formData.allowTerms.$errors[0].$message }}\n            </span>\n        </div>\n        <button name=\"submit\" type=\"submit\" class=\"btn btn-primary\" @click.prevent=\"submitForm\">Send Message</button>\n        <p v-if=\"submitted\" class=\"mt-4 font-14 text-success\">Thanks! for being with us</p>\n    </form>\n</template>\n\n<script>\n    import axios from 'axios'\n    import useVuelidate from '@vuelidate/core'\n    import {required, email} from '@vuelidate/validators'\n\n    export default {\n        name: 'ContactForm',\n        data() {\n            return {\n                v$: useVuelidate(),\n                formData: {\n                    name: '',\n                    email: '',\n                    subject: '',\n                    message: '',\n                    allowTerms: false\n                },\n                submitted: false\n            }\n        },\n        validations() {\n            return {\n                formData: {\n                    name: {required},\n                    email: {required, email},\n                    subject: {required},\n                    message: {required},\n                    allowTerms: {required}\n                }\n            }\n        },\n        methods: {\n            submitForm() {\n                this.v$.$validate();\n                const data = this.formData;\n\n                if (!this.v$.$error) {\n                    axios({\n                        method: 'post',\n                        url: 'https://getform.io/f/7a6695a7-c8e3-442c-bc2f-d46d3b9a535e',\n                        data,\n                    }).then((_res) => {\n                        this.submitted = true;\n                        this.formData.name = '';\n                        this.formData.email = '';\n                        this.formData.subject = '';\n                        this.formData.message = '';\n                        this.formData.allowTerms = false;\n                        this.v$.$reset()\n                    }).catch((err) => {\n                        console.log(err)\n                    });\n                } else {\n                    console.log('Please fill up the required fields')\n                }\n            }\n        }\n    }\n</script>"]}]}