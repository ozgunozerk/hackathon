,\n            countOfPage: {\n                type: Number,\n                default: 8\n            },\n            paginationClass: {\n                type: String\n            },\n            scrollControl: {\n                type: Boolean,\n                default: false\n            }\n        },\n        data() {\n            return {\n                currPage: 1\n            }\n        },\n        computed: {\n            filteredRows() {\n                return this.posts;\n            },\n            pageStart() {\n                return (this.currPage - 1) * this.countOfPage;\n            },\n            totalPage() {\n                return Math.ceil(this.filteredRows.length / this.countOfPage);\n            }\n        },\n        methods: {\n            setPage(idx) {\n                if (idx <= 0 || idx > this.totalPage) {\n                    return;\n                }\n                this.currPage = idx;\n                if (this.scrollControl) {\n                    this.$emit('scrollControl');\n                } else {\n                    window.scrollTo(0, 0);\n                }\n                this.$emit('paginatedData', this.filteredRows, this.pageStart, this.countOfPage);\n            },\n        },\n        mounted() {\n            this.$emit('paginatedData', this.filteredRows, this.pageStart, this.countOfPage)\n        }\n    }\n</script>\n"]}]}