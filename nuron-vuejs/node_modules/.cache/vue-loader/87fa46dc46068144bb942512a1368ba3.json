istener('scroll', updateProgress);\n\n            let offset = 50;\n\n            window.addEventListener('scroll', () => {\n                if (window.scrollY > offset) {\n                    progressParent.classList.add('rn-backto-top-active');\n                } else {\n                    progressParent.classList.remove('rn-backto-top-active');\n                }\n            });\n        }\n    }\n</script>"]}]}